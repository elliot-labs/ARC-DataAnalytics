<?xml version='1.0' encoding='utf-8'?>
<widget id="com.elliot_labs.arcproductioncollection" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ProdCollect</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="ehuffman@elliot-labs.com" href="https://elliot-labs.com">
        Elliot Labs
    </author>
    <icon density="ldpi" height="57" platform="android" src="res/android/icon-36-ldpi.png" width="57" />
    <icon density="mdpi" height="57" platform="android" src="res/android/icon-48-mdpi.png" width="57" />
    <icon density="hdpi" height="57" platform="android" src="res/android/icon-72-hdpi.png" width="57" />
    <icon density="xhdpi" height="57" platform="android" src="res/android/icon-96-xhdpi.png" width="57" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
